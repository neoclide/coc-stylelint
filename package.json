{
  "name": "coc-stylelint",
  "version": "1.0.8",
  "description": "stylelint extension for coc.nvim",
  "main": "lib/index.js",
  "publisher": "chemzqm",
  "engines": {
    "coc": "^0.0.15"
  },
  "keywords": [
    "coc.nvim",
    "stylelint"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "webpack",
    "prepare": "npx npm-run-all clean build"
  },
  "activationEvents": [
    "onLanguage:css",
    "onLanguage:wxss",
    "onLanguage:scss",
    "onLanguage:less",
    "onLanguage:postcss",
    "onLanguage:sugarss",
    "onLanguage:vue"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Stylelint",
      "properties": {
        "stylelint.enable": {
          "type": "boolean",
          "default": true
        },
        "stylelint.execArgv": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          }
        },
        "stylelint.config": {
          "type": [
            "object",
            "null"
          ],
          "default": null,
          "description": "A stylelint configuration object."
        },
        "stylelint.configOverrides": {
          "type": "object",
          "default": {},
          "description": "Override exist stylelint config"
        }
      }
    }
  },
  "author": "chemzqm@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@chemzqm/tsconfig": "^0.0.3",
    "@chemzqm/tslint-config": "^1.0.17",
    "@types/node": "^10.12.15",
    "array-to-error": "^1.1.1",
    "array-to-sentence": "^2.0.0",
    "coc.nvim": "^0.0.39",
    "inspect-with-kind": "^1.0.5",
    "lodash": "^4.17.11",
    "rimraf": "^2.6.2",
    "stylelint-warning-to-vscode-diagnostic": "^1.0.1",
    "ts-loader": "^6.0.3",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "vscode-languageserver": "^5.2.1",
    "vscode-languageserver-protocol": "^3.14.1",
    "vscode-uri": "^1.0.6",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "which": "^1.3.1"
  },
  "dependencies": {
    "postcss": "^7.0.6",
    "postcss-html": "^0.34.0",
    "postcss-jsx": "^0.35.0",
    "postcss-markdown": "^0.34.0",
    "postcss-syntax": "^0.34.0",
    "stylelint": "^9.9.0"
  }
}
